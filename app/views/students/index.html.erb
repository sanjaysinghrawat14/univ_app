<div class="row">
<div class="col s12">

<h3 class="center-align">Students Listing</h3>
</div></div>

<% @students.each do |student| %>
<div class="row">
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <p >Name: <%= student.name%></p>
          <p >Email: <%= student.email  %></p>

          <p><%= "Course Enrollment : None" %></p>
        </div>
        <div class="card-action">
          <%= link_to "View Profile", student%>
        </div>
      </div>
    </div>
<%end%>



